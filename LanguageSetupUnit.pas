  If not FileExists(FileName) then FileName:='';
  If FileName='' then begin
    ForceDirectories(PrgDataDir+LanguageSubDir);
    inherited Create(PrgDataDir+LanguageSubDir+'\'+DefaultLanguageFile);
  end else begin
    inherited Create(FileName);
  end;

  if MinimalInit then begin
    StoreConfigOnExit:=False;
  end else begin
    SetChanged;
  end;

